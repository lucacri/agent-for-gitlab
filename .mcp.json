{
	"mcpServers": {
		"vibe_kanban": {
			"command": "npx",
			"args": [
				"-y",
				"vibe-kanban",
				"--mcp"
			]
		},
		"gemini-cli": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"gemini-mcp-tool"
			],
			"env": {}
		},
		"tavily": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"tavily-mcp"
			],
			"env": {
				"TAVILY_API_KEY": "${TAVILY_API_KEY}"
			}
		},
		"sequentialthinking": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			]
		},
		"codex": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@openai/codex",
				"--full-auto",
				"--search",
				"mcp-server"
			],
			"env": {}
		}
	}
}